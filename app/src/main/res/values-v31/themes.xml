<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DynamicColorsBaseTheme" parent="Theme.Material3.DynamicColors.DayNight.NoActionBar">
        <item name="navEnterAnim">@anim/nav_slide_enter_anim</item>
        <item name="navExitAnim">@anim/nav_fade_exit_anim</item>
        <item name="navPopEnterAnim">@anim/nav_fade_enter_anim</item>
        <item name="navPopExitAnim">@anim/nav_slide_exit_anim</item>

        <item name="materialAlertDialogTheme">@style/ThemeOverlay.App.MaterialAlertDialog</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="actionModeCloseDrawable">@drawable/ic_close_24px</item>
        <item name="actionBarTheme">@style/ThemeOverlay.Material3.ActionBar</item>
        <item name="actionModeBackground">?attr/colorSurfaceContainer</item>
        <item name="materialSearchBarStyle">@style/Widget.App.SearchBar</item>
        <item name="appBarLayoutStyle">@style/Widget.App.AppBarLayout</item>
        <item name="preferenceTheme">@style/App.PreferenceThemeOverlay</item>
    </style>
    <style name="AppTheme" parent="DynamicColorsBaseTheme"/>
    <style name="AppTheme.NoDynamicColors" parent="BaseTheme"/>
    <style name="AppTheme.NoDynamicColors.Black"/>
</resources>